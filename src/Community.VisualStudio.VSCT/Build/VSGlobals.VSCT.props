<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0">
  <PropertyGroup>
    <VSGlobalsVSCTPath Condition="'$(VSGlobalsVSCTPath)'==''">$([System.IO.Path]::GetFullPath($([System.IO.Path]::Combine($(MSBuildThisFileDirectory),'..\vsct\'))))</VSGlobalsVSCTPath>
    <ShowVSGlobalsVSCT Condition="'$(ShowVSGlobalsVSCT)'==''">false</ShowVSGlobalsVSCT>
    <EnableVSGlobalsVSCT Condition="'$(EnableVSGlobalsVSCT)'==''">true</EnableVSGlobalsVSCT>
    <VSGlobalsVSCTDependentUpon Condition="'$(VSGlobalsVSCTDependentUpon)'==''">VSCommandTable.vsct</VSGlobalsVSCTDependentUpon>
  </PropertyGroup>
</Project>